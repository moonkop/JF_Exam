<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="com.njmsita.exam.authentic.model.TresourceTypeModel" table="tresourcetype">
        <id name="id">
            <generator class="assigned"/>
        </id>
        <properties name="name"/>

        <many-to-one name="resourceList" column="tresourcetype_id" class="com.njmsita.exam.authentic.model.TresourceModel"/>
    </class>
</hibernate-mapping>
