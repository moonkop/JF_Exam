<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="com.njmsita.exam.manager.model.ExamModel" table="exam">
        <id name="id">
            <generator class="assigned"/>
        </id>
        <properties name="duration"/>
        <properties name="paper_content"/>
        <properties name="classroom_id"/>
        <properties name="open_time"/>
        <properties name="close_time"/>
        <properties name="exam_status"/>
    </class>
</hibernate-mapping>
