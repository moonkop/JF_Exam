<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.njmsita.exam.manager.model.LogVo" table="log" schema="jf_exam">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="userId" column="userId"/>
        <property name="module" column="module"/>
        <property name="method" column="method"/>
        <property name="ip" column="ip"/>
        <property name="time" column="time"/>
        <property name="commite" column="commite"/>
        <property name="responseTime" column="response_time"/>
    </class>
</hibernate-mapping>