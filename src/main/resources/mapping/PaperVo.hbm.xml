<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.njmsita.exam.manager.model.PaperVo" table="paper" schema="jf_exam">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="comment" column="comment"/>
        <property name="questionContain" column="questionContain"/>

        <many-to-one name="teacher" column="create_teacher_id" class="com.njmsita.exam.authentic.model.TeacherVo"/>
        <many-to-one name="subjectVo" column="subject_id" class="com.njmsita.exam.manager.model.SubjectVo"/>
    </class>
</hibernate-mapping>