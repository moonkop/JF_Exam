<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.njmsita.exam.manager.model.QuestionVo" table="question" schema="jf_exam">
        <id name="id" column="id">
            <generator class="assigned"/>
        </id>
        <property name="isPrivate" column="is_private"/>
        <property name="createTime" column="create_time"/>
        <property name="code" column="code"/>
        <property name="outline" column="outline"/>
        <property name="option" column="option"/>
        <property name="answer" column="answer"/>
        <property name="topicId" column="topic_id"/>
        <property name="createTeacherId" column="create_teacher_id"/>
        <property name="questionTypeId" column="question_type_id"/>
        <property name="subjectId" column="subject_id"/>
    </class>
</hibernate-mapping>