<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.njmsita.exam.manager.model.StudentExamQuestionVo" table="student_exam_question" schema="jf_exam">
        <id name="id" column="id"/>
        <property name="indext" column="indext"/>
        <property name="score" column="score"/>
        <property name="remark" column="remark"/>
        <property name="answer" column="answer"/>

        <many-to-one name="studentExamVo" column="studentexam_id" class="com.njmsita.exam.manager.model.StudentExamVo"/>
        <many-to-one name="teacherVo" column="teacher_id" class="com.njmsita.exam.authentic.model.TeacherVo"/>
        <many-to-one name="questionTypeVo" column="questionType" class="com.njmsita.exam.manager.model.QuestionTypeVo"/>
    </class>
</hibernate-mapping>