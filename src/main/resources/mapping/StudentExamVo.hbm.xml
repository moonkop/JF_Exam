<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.njmsita.exam.manager.model.StudentExamVo" table="student_exam" schema="jf_exam">
        <id name="id" column="id"/>
        <property name="startTime" column="start_time"/>
        <property name="submitTime" column="submit_time"/>
        <property name="score" column="score"/>
        <property name="answerContent" column="answer_content"/>
        <property name="operation" column="operation"/>

        <many-to-one name="examVo" column="exam_id" class="com.njmsita.exam.manager.model.ExamVo"/>
        <many-to-one name="studentVo" column="student_id" class="com.njmsita.exam.authentic.model.StudentVo"/>
    </class>
</hibernate-mapping>